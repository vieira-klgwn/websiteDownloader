$('.tabs_links li').on('click', function(){
    var activeID = $(this).attr('id');
    $('.tabs_links li a').removeClass('active');
    $('a', $(this)).addClass('active');
    $('.tabs_item').removeClass('active_tab');
    $('#content-' + activeID).addClass('active_tab');
});
