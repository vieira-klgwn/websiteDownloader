      <script>
    function ntSectionSlider() {
        $('.nt-section-slider #newsCarousel').owlCarousel({
            loop: false,
            navRewind: false,
            margin: 0,
            items: 1,
            slideBy: 1,
            nav: true,
            dots: true,
            dotClass: "owl-page",
            dotsClass: "owl-pagination",
            autoplayHoverPause: false,
            smartSpeed: 3000,
            navText: ['<span class="icon-triangle-left"></span>', '<span class="icon-triangle-right"></span>'],
        });
    }

    var base_url = $('#BASE_URL').val();
    if (typeof ($(".nt-section-slider #newsCarousel").owlCarousel) === "undefined") {
        $.getScript(base_url + "js/owl.carousel.min.js", function () {
            ntSectionSlider();
            setNumbers();
        })
    } else {
        ntSectionSlider();
        setNumbers();
    }
    function setNumbers(){
         $('.owl-page').each(function () {
            $(this).children('span').text($(this).index() + 1);
        });
        if($(window).width() < 768){
            var firstItemPosition = $('.owl-pagination .owl-page:first-child').position(), lastItemPosition = $('.owl-pagination .owl-page:last-child').position();
            $('.nt-section-slider .owl-theme .owl-nav .owl-next').css('left', parseInt(lastItemPosition.left + 35) + "px");
            $('.nt-section-slider .owl-theme .owl-nav .owl-prev').css('left', parseInt(firstItemPosition.left - 32) + "px");
        }
    }
</script>
